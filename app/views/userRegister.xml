 passwordMask=true<Alloy>
	<Window id="userRegister">
		<View id = "mainView" backgroundColor="white"  layout="vertical">
			<View id = "topView" backgroundColor="white" top ="0" height ="50" width ="100%">
				<Label id = "titleLable" bottom = "0" height="50"  color="black">User Register</Label>
				<Label id = "logoutLable" bottom = "0"  onClick="logoutClicked">Logout</Label>
			</View>
			<View id = "lineView" backgroundColor="#5F93A9" height="1" width="100%" top="0"></View>
			    <Label id = "userName" class="labelProperty" text="User Name"></Label>
				<TextField id = "userNameField" class="textFiledProperty"></TextField>
				<Label id = "email" class="labelProperty" text="Email"></Label>
				<TextField id = "emailField" class="textFiledProperty"></TextField>
				<Label id = "password" class="labelProperty" text="Password"></Label>
				<TextField id = "passwordField" class="textFiledProperty" passwordMask=true></TextField>
				<Label id = "confirmPassword" class="labelProperty" text="Confirm Password" ></Label>
				<TextField id = "confirmPasswordField" class="textFiledProperty" passwordMask=true></TextField>
				<Button id = "Submit" onClick="submitButtonClicked"></Button>
		</View>
	</Window>
