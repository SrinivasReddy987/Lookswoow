<Alloy>
	<Window id="congrats" layout="vertical">
		<View id = "topView" backgroundColor="white" top ="0" height ="60" width ="100%">
			<Label id = "titleLable" bottom = "0" height="80%">Appoinment Sucessful</Label>
			<View id = "lineView" backgroundColor="black" height="0.5" width="100%" bottom="0"></View>
			<Label id = "backLabel"  text="< Back" onClick="goBack" left = "3" top = "45%"></Label>
			<Label id = "logoutLable" bottom = "0"  onClick="logoutClicked">Logout</Label>
		</View>
		<ImageView id = "congratsImage" top="20" height="35%" image="/star/congrats.png"></ImageView>
		<Label id = "congrtasLabel" top = "10" text = "Your Appoinment has been confirmed"></Label>
		<ImageView id = "appointmentListImage" onClick="viewAppointmentList"></ImageView>
		<View id = "bottomView" bottom= "0">
			<Require id="tabView" src="tabView" />
		</View>  
    </Window>
</Alloy>
